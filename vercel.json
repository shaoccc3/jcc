{
  "version": 2,
  "builds": [
    {
      "src": "public/bd.html",
      "use": "@vercel/static",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/bd",
      "dest": "/public/bd.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
